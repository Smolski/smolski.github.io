@article{Albuquerque2015,
abstract = {O presente relato de experi{\^{e}}ncia apresenta a concep{\c{c}}{\~{a}}o de extens{\~{a}}o de um projeto da Universidade Federal do Paran{\'{a}}, intitulado “Integra{\c{c}}{\~{a}}o Ensino/Servi{\c{c}}o/Comunidade no SUS”, entendendo-a como componente do trip{\'{e}} ensino/pesquisa/extens{\~{a}}o, constituintes indissoci{\'{a}}veis do processo de forma{\c{c}}{\~{a}}o. Com base nessa compreens{\~{a}}o, descreve as atividades realizadas, buscando demonstrar a coer{\^{e}}ncia da a{\c{c}}{\~{a}}o pr{\'{a}}tica com seu fundamento te{\'{o}}rico. O projeto parte da avalia{\c{c}}{\~{a}}o de servi{\c{c}}os de sa{\'{u}}de da aten{\c{c}}{\~{a}}o b{\'{a}}sica do SUS, com a identifica{\c{c}}{\~{a}}o de problemas, formula{\c{c}}{\~{a}}o de propostas de solu{\c{c}}{\~{o}}es e implanta{\c{c}}{\~{a}}o de altera{\c{c}}{\~{o}}es nos servi{\c{c}}os visando sua melhoria. Tais a{\c{c}}{\~{o}}es proporcionam aos estudantes e trabalhadores da sa{\'{u}}de um processo de aprendizagem que parte da representa{\c{c}}{\~{a}}o sincr{\'{e}}tica da realidade, passa por momentos de abstra{\c{c}}{\~{a}}o e retorna {\`{a}} realidade num movimento de s{\'{i}}ntese integradora, potencializadora da compreens{\~{a}}o dos fen{\^{o}}menos para al{\'{e}}m da sua apar{\^{e}}ncia, reafirmando a realidade como crit{\'{e}}rio de verdade do conhecimento produzido. Os usu{\'{a}}rios s{\~{a}}o, tamb{\'{e}}m, beneficiados pela melhor compreens{\~{a}}o da realidade e pelo aprimoramento do atendimento dos servi{\c{c}}os prestados pelas equipes de sa{\'{u}}de resultante de tal movimento. As experi{\^{e}}ncias t{\^{e}}m sido documentadas e divulgadas atrav{\'{e}}s de apresenta{\c{c}}{\~{o}}es em eventos cient{\'{i}}ficos e publica{\c{c}}{\~{o}}es de artigos em peri{\'{o}}dicos.},
