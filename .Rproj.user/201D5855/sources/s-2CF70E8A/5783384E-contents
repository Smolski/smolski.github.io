@article{Meireles2011,
abstract = {An{\'{a}}lises de conte{\'{u}}do e de cita{\c{c}}{\~{o}}es s{\~{a}}o metodologias que se valem da infer{\^{e}}ncia e da interpreta{\c{c}}{\~{a}}o para obter informa{\c{c}}{\~{o}}es relevantes contidas em um conjunto de documentos. Estes m{\'{e}}todos s{\~{a}}o amplamente discutidos na literatura, mas s{\~{a}}o poucos os artigos encontrados nos quais se descreve e exemplifica sua aplica{\c{c}}{\~{a}}o em estudos de caso espec{\'{i}}ficos. Este artigo apresenta um estudo de caso que tem o duplo prop{\'{o}}sito de demonstrar o uso dos m{\'{e}}todos citados para an{\'{a}}lise de um conjunto de documentos e de mostrar o potencial da sua utiliza{\c{c}}{\~{a}}o conjunta para a recupera{\c{c}}{\~{a}}o de informa{\c{c}}{\~{a}}o. O artigo apresenta, inicialmente, as abordagens te{\'{o}}ricas relativas aos dois m{\'{e}}todos e a caracteriza{\c{c}}{\~{a}}o do material emp{\'{i}}rico da pesquisa, que utilizou artigos sobre Redes Neurais Artificiais. A seguir, s{\~{a}}o descritas as categorias sem{\^{a}}nticas utilizadas na an{\'{a}}lise de conte{\'{u}}do e as vari{\'{a}}veis selecionadas para a an{\'{a}}lise de cita{\c{c}}{\~{o}}es. Por meio de an{\'{a}}lises quantitativas e qualitativas, exemplifica-se o uso da an{\'{a}}lise de cita{\c{c}}{\~{o}}es e da an{\'{a}}lise de conte{\'{u}}do e demonstra-se, ainda, que elas podem ser utilizadas de modo integrado para a obten{\c{c}}{\~{a}}o de metadados a respeito de cole{\c{c}}{\~{o}}es de documentos.},
author = {Meireles, Magali Rezende Gouvea and Cendon, Beatriz Valadares},
doi = {10.5433/1981-8920.2010v15n2p77},
