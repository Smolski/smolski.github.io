@article{Zhang2013,
abstract = {In this paper, we study the U.S. aggregate business failures during 1980- 2004 in relation to four macroeconomic variables: aggregate corporate profits, the producer price index, the interest rate, and stock market performance. We argue that aggregate business failures should not be treated as a passive variable, as usually done in previous studies, and we allow its possible causal effect on other macroeconomic variables through a Structural Vector Autoregression model that builds on Directed Acyclic Graphs. Granger type causality and innovation accounting results both show that while subject to the influence of interest rates, aggregate business failures are quite exogenous in comparison to the other three variables. The implications of these findings are discussed as well. {\textcopyright} 2013 Universidad del CEMA.},
author = {Zhang, Jin and Bessler, David A. and Leatham, David J.},
doi = {10.1016/S1514-0326(13)60008-2},
file = {:C$\backslash$:/Users/Felipe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhang, Bessler, Leatham - 2013 - Aggregate business failures and macroeconomic conditions A var look at the U.S. between 1980 and 2004.pdf:pdf},
issn = {15140326},
journal = {Journal of Applied Economics},
keywords = {Business failures,Directed acyclic graphs,Macroeconomic conditions,Vector autoregression},
number = {1},
pages = {179--202},
publisher = {Universidad del CEMA},
