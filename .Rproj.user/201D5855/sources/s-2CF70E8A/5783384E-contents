@article{Engelman2012,
abstract = {Este texto apresenta um novo modelo de gest{\~{a}}o p{\'{u}}blica que tem como base as transforma{\c{c}}{\~{o}}es organizacionais que ocorrem no setor privado, trazendo propostas que alteram a forma burocr{\'{a}}tico-piramidal de administra{\c{c}}{\~{a}}o, flexibilizando a gest{\~{a}}o, diminuindo os n{\'{i}}veis hier{\'{a}}rquicos e aumentando a autonomia de decis{\~{a}}o de gestores. A descentraliza{\c{c}}{\~{a}}o apresenta-se como uma estrat{\'{e}}gia de moderniza{\c{c}}{\~{a}}o do Estado que busca simultaneamente prover o processo democr{\'{a}}tico e participativo e atingir maior efici{\^{e}}ncia. Neste sentido encontra-se o Estado-Rede, que segundo Castells (1999a), se caracteriza por compartilhar autoridade e pela emana{\c{c}}{\~{a}}o da pluralidade das fontes de autoridade por meio de uma s{\'{e}}rie de institui{\c{c}}{\~{o}}es. Esta rede possibilita a inova{\c{c}}{\~{a}}o concreta na consolida{\c{c}}{\~{a}}o de pol{\'{i}}ticas p{\'{u}}blica. Assim, o Programa Segundo Tempo (PST) da Secretaria Nacional de Esporte Educacional {\'{e}} uma pol{\'{i}}tica p{\'{u}}blica que exemplifica os novos referenciais de gest{\~{a}}o. Este programa contribui para a democratiza{\c{c}}{\~{a}}o do esporte, diminui{\c{c}}{\~{a}}o das situa{\c{c}}{\~{o}}es de risco social dos alunos, al{\'{e}}m de capacitar professores de Educa{\c{c}}{\~{a}}o F{\'{i}}sica, monitores e estudantes de gradua{\c{c}}{\~{a}}o, objetivando a gest{\~{a}}o p{\'{u}}blica ampliada. Uniram-se ao PST, a Universidade Estadual de Maring{\'{a}} e Universidade Federal do Rio Grande do Sul, com o intuito de ser elaborada uma proposta de suporte te{\'{o}}rico e pr{\'{a}}tico para as a{\c{c}}{\~{o}}es do Programa. O PST conta com as equipes colaboradoras, as quais s{\~{a}}o compostas por um coordenador, um vice-coordenador e avaliadores. Desta forma, construir pol{\'{i}}ticas p{\'{u}}blicas no Estado exige todo um trabalho de conex{\~{a}}o com esfor{\c{c}}os do coletivo, com os movimentos sociais e com pr{\'{a}}ticas concretas do cotidiano.},
author = {Engelman, Selda and {De Oliveira}, Amauri Aparecido Bassoli},
doi = {10.4025/reveducfis.v23.4.13379},
file = {:C$\backslash$:/Users/Felipe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Engelman, De Oliveira - 2012 - Gest{\~{a}}o p{\'{u}}blica em rede O caso do programa segundo tempo - Minist{\'{e}}rio do esporte.pdf:pdf},
issn = {01033948},
journal = {Revista da Educa{\c{c}}{\~{a}}o Fisica},
keywords = {Management,Public policy,Sport},
number = {4},
