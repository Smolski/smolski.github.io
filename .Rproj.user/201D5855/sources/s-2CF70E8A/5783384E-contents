@article{Melicher1988,
abstract = {The finance literature contains many studies that focus on explaining and/or predicting individual failures using firm-specific finacial data. In contrast there has been only a very modest amount of research into the relationships between aggregate failure activity and macroeconomic/market conditions. These studies, in addition, provide very limited insights into the structural (especially lead-lag) relationships between failure activity and macroeconomic/market conditions. This study employs a data-based multiple time series approach to develop an explanatory model for describing changes in aggregate failure activity relative to changes in financial markets variables during the 1950 through 1983 time period. It is our contention that failures are essentially a credit or money market conditions phenomenon. Results suggest that aggregate failure activity lags behind the volatility of interest rates and the cost of short-term credit. Furthermore, there is some evidence that aggregate failure activity also lags behind the availability of short-term credit. ?? 1988.},
author = {Melicher, Ronald W. and Hearth, Douglas},
doi = {10.1016/0148-6195(88)90030-6},
file = {:C$\backslash$:/Users/Felipe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Melicher, Hearth - 1988 - A time series analysis of aggregate business failure activity and credit conditions.pdf:pdf},
isbn = {0148-6195},
issn = {01486195},
journal = {Journal of Economics and Business},
number = {4},
pages = {319--333},
title = {{A time series analysis of aggregate business failure activity and credit conditions}},
volume = {40},
year = {1988}
