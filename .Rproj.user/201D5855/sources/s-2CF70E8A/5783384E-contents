@article{BenderFilho2012,
abstract = {We investigated the relative importance of technological and mone- tary shocks for the business cycles in the Brazilian economy between 1980 and 2009. In this investigation, we used the model money-in-the-utility function, which considers that holdingmoney generates utility to the eco- nomic agents. Several simulations were performed to identify how the business cycles respond to shocks. The results indicate that, in addition to technological shocks, monetary shocks are important in explaining busi- ness cycles in the Brazilian economy during the analyzed period. Their effects on the volatility of real variables, however, were small.},
