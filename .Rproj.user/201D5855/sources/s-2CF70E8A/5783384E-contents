@article{Crocco2014,
abstract = {Banks and regional development: an empirical analysis on the determinants of credit availability in Brazilian regions, Regional Studies. This paper studies the influence that liquidity preference and bank lending strategies might have played in the determination of bank credit availability in Brazilian regions during the period 1999–2008. It puts forward the argument that in remoter and less developed regions, the liquidity pre- ference of agents is higher and fluctuates along business cycles, reducing regional credit availability in downturns (due to both a contraction in supply and the demand for credit in remoter regions), and producing a more unstable pattern for credit availability alongside business cycles. Empirical evidence for the Brazilian regions during the period 1999–2008 is provided to support this},
author = {Crocco, Marco and Faria-Silva, Fernanda and Paulo-Rezende, Luiz and Rodr{\'{i}}guez-Fuentes, Carlos J.},
