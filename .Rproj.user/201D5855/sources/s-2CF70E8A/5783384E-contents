@misc{Silva2014,
abstract = {Resumo
O presente estudo tem por objetivo esclarecer os pressupostos te{\'{o}}ricos que embasam os argumentos distintos nas diversas correntes econ{\^{o}}micas, dentro do debate da desindustrializa{\c{c}}{\~{a}}o brasileira. O processo de desindustrializa{\c{c}}{\~{a}}o pode ocorrer de forma natural ou precoce; por{\'{e}}m, no caso da economia brasileira, n{\~{a}}o h{\'{a}} um consenso sobre o tipo de desindustrializa{\c{c}}{\~{a}}o em voga. Existe uma pluralidade de teses em rela{\c{c}}{\~{a}}o {\`{a}} exist{\^{e}}ncia, ao per{\'{i}}odo, {\`{a}}s causas, {\`{a}}s consequ{\^{e}}ncias, entre outros fatores, desse processo. A diverg{\^{e}}ncia ou a aproxima{\c{c}}{\~{a}}o de argumentos entre os autores nesse debate n{\~{a}}o corresponde, necessariamente, {\`{a}} determinada linha te{\'{o}}rica a partir de uma avalia{\c{c}}{\~{a}}o realizada com base nos indicadores cl{\'{a}}ssicos de desindustrializa{\c{c}}{\~{a}}o.
 
Palavras-chave: desindustrializa{\c{c}}{\~{a}}o; ind{\'{u}}stria de transforma{\c{c}}{\~{a}}o; taxa de c{\^{a}}mbio.

T{\'{I}}TULO EM INGL{\^{E}}S
Deindustrialization in debate: theses and misconceptions in the case of the Brazilian economy

Abstract
The present study aims at explaining the theoretical assumptions behind the distinct arguments in several economic approaches within the debate on Brazilian industrialization. The process of deindustrialization can occur naturally or precociously; however in the case of the Brazilian economy, there is no consensus on the type of existing deindustrialization. There is a plurality of theses about the existence, period, causes, consequences, and other factors involved in that process. The divergence or convergence of arguments among authors in this debate does not necessarily correspond to a particular theoretical line which stems from an evaluation based on the classical indicators of deindustrialization.

Key words: deindustrialization; manufacturing; exchange rate.

Artigo recebido em 28 de maio de 2014.},
author = {da Silva, Jose Alderir and de Louren{\c{c}}o, Andr{\'{e}} Lu{\'{i}}s Cabral},
booktitle = {Indicadores Econ{\^{o}}micos FEE},
file = {:C$\backslash$:/Users/Felipe/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Silva, Louren{\c{c}}o - 2014 - Desindustrializa{\c{c}}{\~{a}}o em debate teses e equ{\'{i}}vocos no caso da economia brasileira.pdf:pdf},
issn = {1806-8987},
language = {pt},
month = {nov},
number = {2},
pages = {57--76},
title = {{Desindustrializa{\c{c}}{\~{a}}o em debate: teses e equ{\'{i}}vocos no caso da economia brasileira}},
url = {http://revistas.fee.tche.br/index.php/indicadores/article/view/3382},
volume = {42},
year = {2014}
}
@article{Bresser-Pereira2009,
abstract = {The two methods and the hard core of economics. While methodological sciences have no object and are supposed to adopt a hypothetical-deductive method, substantive sciences including economics should use an empirical or historicaldeductive method. The great classical economists and Keynes did that and were able to develop open models explaining how equally open economic systems work. Thus, the hard core of relevant economics is formed by the classical microeconomics and the classical theory of capitalist economic growth, and by Keynesian macroeconomics. In contrast, neoclassical economist aiming to build a mathematical science wrongly adopted the hypothetical-deductive method, and came to macroeconomic and growth models that do not have practical use in policymaking. The exception is Marshall's microeconomics that does not provide a model of real economic systems, but is useful to the analysis of markets.},
