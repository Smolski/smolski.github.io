@article{Gama2016,
abstract = {Resumo: O objetivo do presente trabalho {\'{e}} analisar as rela{\c{c}}{\~{o}}es entre o desempenho regional e vari{\'{a}}veis financeiras para os munic{\'{i}}pios brasileiros, tendo como pano de fundo os conceitos da teoria p{\'{o}}s-keynesiana. Com base na t{\'{e}}cnica de estat{\'{i}}stica multivariada de clusters, foi poss{\'{i}}vel agrupar os munic{\'{i}}pios de acordo com as suas similaridades de desenvolvimento socioecon{\^{o}}mico. Como resultado, obtiveram-se tr{\^{e}}s clusters de munic{\'{i}}pios: Centrais, Intermedi{\'{a}}rios e Perif{\'{e}}ricos. Empregando a t{\'{e}}cnica de dados em painel, avaliaram-se as implica{\c{c}}{\~{o}}es das vari{\'{a}}veis financeiras e seus efeitos sobre o n{\'{i}}vel de atividade dos diferentes clusters de munic{\'{i}}pios. Os resultados para o per{\'{i}}odo de 2000 a 2010 mostraram evid{\^{e}}ncias a favor da hip{\'{o}}tese de exist{\^{e}}ncia de efeitos diferenciados da moeda no sentido p{\'{o}}s-keynesiano sobre o n{\'{i}}vel de atividade dos munic{\'{i}}pios brasileiros. N{\~{a}}o obstante, essas evid{\^{e}}ncias corroboram os resultados encontrados por uma gama de estudos aplicados para a quest{\~{a}}o regional do Brasil.},
