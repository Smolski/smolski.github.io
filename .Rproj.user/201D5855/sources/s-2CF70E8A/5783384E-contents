@article{Dow1987,
abstract = {A survey of regional monetary theory suggests a regional segmentation$\backslash$nof financial markets on the basis of differences in portfolio preference,$\backslash$nor market imperfections. These possibilities are explored here using$\backslash$npost-Keynesian monetary theory. It is concluded that the availability$\backslash$nof credit to a region depends more on the (potentially volatile)$\backslash$ndegree of confidence in the regional economy. The amount of liquidity$\backslash$nthus generated depends on the propensity for capital flight due to$\backslash$nliquidity preference. The resulting tendency for regions attracting$\backslash$nlow degrees of confidence to experience liquidity shortage will be$\backslash$ngreater the more integrated the national banking system.},
