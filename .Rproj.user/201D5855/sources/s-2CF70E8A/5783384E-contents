@article{Caldas2004,
abstract = {Neste artigo {\'{e}} usada an{\'{a}}lise bibliom{\'{e}}trica para entender a influ{\^{e}}ncia de autores e institui{\c{c}}{\~{o}}es na produ{\c{c}}{\~{a}}o acad{\^{e}}mica em Recursos Humanos no Brasil na d{\'{e}}cada de 1990, bem como para mapear o comportamento de auto-cita{\c{c}}{\~{o}}es e cita{\c{c}}{\~{o}}es {\`{a}} pr{\'{o}}pria institui{\c{c}}{\~{a}}o dos autores que produziram no per{\'{i}}odo. O estudo levantou, nos 290 artigos de RH publicados na d{\'{e}}cada de 1990 (1991 a 2000) nos anais do Enanpad, a constitui{\c{c}}{\~{a}}o, origem e padr{\~{a}}o de interrela{\c{c}}{\~{a}}o de todas as cita{\c{c}}{\~{o}}es, autores e institui{\c{c}}{\~{o}}es que publicaram e foram citados na {\'{a}}rea. No total, analisou-se os padr{\~{o}}es bibliom{\'{e}}tricos de 5.814 cita{\c{c}}{\~{o}}es, que compreendiam 342 autores, advindos de 51 institui{\c{c}}{\~{o}}es. Os resultados mostram uma {\'{a}}rea com alto {\'{i}}ndice de auto-cita{\c{c}}{\~{a}}o e endogenia, e com uma elevada incid{\^{e}}ncia de cita{\c{c}}{\~{a}}o de jornais, revistas e autores estrangeiros e n{\~{a}}o acad{\^{e}}micos. O artigo termina oferecendo ao campo sugest{\~{o}}es para desenvolvimento e debate, a partir dos seus resultados e sua compara{\c{c}}{\~{a}}o com trabalhos anteriores analisando a mesma base amostral.},
